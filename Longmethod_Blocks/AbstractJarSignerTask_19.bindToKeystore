void#bindToKeystore#(#final#ExecTask#cmd#)#{##if#(#null#!=#keystore#)#{##// is the keystore a file#addValue#(#cmd#,#"-keystore"#)#;#String#loc#;#File#keystoreFile#=#getProject#(#)#.#resolveFile#(#keystore#)#;##if#(#keystoreFile#.#exists#(#)#)#{#loc#=#keystoreFile#.#getPath#(#)#;#}#else#{##// must be a URL - just pass as is#loc#=#keystore#;#}##addValue#(#cmd#,#loc#)#;#}##if#(#null#!=#storetype#)#{#addValue#(#cmd#,#"-storetype"#)#;#addValue#(#cmd#,#storetype#)#;#}#}