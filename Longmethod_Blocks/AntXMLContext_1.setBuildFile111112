void#setBuildFile#(#File#buildFile#)#{#this#.#buildFile#=#buildFile#;##if#(#buildFile#!=#null#)#{#this#.#buildFileParent#=#new#File#(#buildFile#.#getParent#(#)#)#;#implicitTarget#.#setLocation#(#new#Location#(#buildFile#.#getAbsolutePath#(#)#)#)#;##try#{#setBuildFile#(#FileUtils#.#getFileUtils#(#)#.#getFileURL#(#buildFile#)#)#;#}##catch#(#MalformedURLException#ex#)#{#throw#new#BuildException#(#ex#)#;#}#}#else#{#this#.#buildFileParent#=#null#;#}#}