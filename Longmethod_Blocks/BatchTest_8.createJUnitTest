JUnitTest#createJUnitTest#(#String#classname#)#{#JUnitTest#test#=#new#JUnitTest#(#)#;#test#.#setName#(#classname#)#;#test#.#setHaltonerror#(#this#.#haltOnError#)#;#test#.#setHaltonfailure#(#this#.#haltOnFail#)#;#test#.#setFiltertrace#(#this#.#filtertrace#)#;#test#.#setFork#(#this#.#fork#)#;#test#.#setIf#(#getIfCondition#(#)#)#;#test#.#setUnless#(#getUnlessCondition#(#)#)#;#test#.#setTodir#(#this#.#destDir#)#;#test#.#setFailureProperty#(#failureProperty#)#;#test#.#setErrorProperty#(#errorProperty#)#;#Enumeration#list#=#this#.#formatters#.#elements#(#)#;##while#(#list#.#hasMoreElements#(#)#)#{#test#.#addFormatter#(#(#FormatterElement#)#list#.#nextElement#(#)#)#;#}##return#test#;#}