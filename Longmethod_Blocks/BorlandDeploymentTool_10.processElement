void#processElement#(#)#{##if#(#currentElement#.#equals#(#"type-storage"#)#)#{##// Get the filename of vendor specific descriptor#String#fileNameWithMETA#=#currentText#;##//trim the META_INF\ off of the file name#String#fileName#=#fileNameWithMETA#.#substring#(#META_DIR#.#length#(#)#,#fileNameWithMETA#.#length#(#)#)#;#File#descriptorFile#=#new#File#(#srcDir#,#fileName#)#;#ejbFiles#.#put#(#fileNameWithMETA#,#descriptorFile#)#;#}#}