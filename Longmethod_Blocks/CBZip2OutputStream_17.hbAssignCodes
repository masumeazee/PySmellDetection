void#hbAssignCodes#(#final#int#[#]#code#,#final#byte#[#]#length#,#final#int#minLen#,#final#int#maxLen#,#final#int#alphaSize#)#{#int#vec#=#0#;##for#(#int#n#=#minLen#;##n#<=#maxLen#;#n#++#)#{#for#(#int#i#=#0#;##i#<#alphaSize#;#i#++#)#{##if#(#(#length#[#i#]#&#0xff#)#==#n#)#{#code#[#i#]#=#vec#;#vec#++#;#}#}##vec#<<=#1#;#}#}