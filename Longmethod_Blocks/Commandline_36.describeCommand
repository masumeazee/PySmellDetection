String#describeCommand#(#String#[#]#args#)#{##if#(#args#==#null#||#args#.#length#==#0#)#{##return#""#;#}##StringBuffer#buf#=#new#StringBuffer#(#"Executing \'"#)#;#buf#.#append#(#args#[#0#]#)#;#buf#.#append#(#"\'"#)#;##if#(#args#.#length#>#1#)#{#buf#.#append#(#" with "#)#;#buf#.#append#(#describeArguments#(#args#,#1#)#)#;#}#else#{#buf#.#append#(#DISCLAIMER#)#;#}##return#buf#.#toString#(#)#;#}