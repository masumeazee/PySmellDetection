Map#getRestrictedDefintion#(#)#{#Map#result#=#new#HashMap#(#)#;##synchronized#(#restrictedDefinitions#)#{#for#(#Iterator#i#=#restrictedDefinitions#.#entrySet#(#)#.#iterator#(#)#;##i#.#hasNext#(#)#;#)#{#Map#.#Entry#entry#=#(#Map#.#Entry#)#i#.#next#(#)#;#List#entryVal#=#(#List#)#entry#.#getValue#(#)#;##synchronized#(#entryVal#)#{##//copy the entryVal#entryVal#=#new#ArrayList#(#entryVal#)#;#}##Object#entryKey#=#entry#.#getKey#(#)#;#result#.#put#(#entryKey#,#entryVal#)#;#}#}##return#result#;#}