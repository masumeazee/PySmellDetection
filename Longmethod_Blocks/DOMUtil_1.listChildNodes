NodeList#listChildNodes#(#Node#parent#,#NodeFilter#filter#,#boolean#recurse#)#{#NodeListImpl#matches#=#new#NodeListImpl#(#)#;#NodeList#children#=#parent#.#getChildNodes#(#)#;##if#(#children#!=#null#)#{#final#int#len#=#children#.#getLength#(#)#;##for#(#int#i#=#0#;##i#<#len#;#i#++#)#{#Node#child#=#children#.#item#(#i#)#;##if#(#filter#.#accept#(#child#)#)#{#matches#.#addElement#(#child#)#;#}##if#(#recurse#)#{#NodeList#recmatches#=#listChildNodes#(#child#,#filter#,#recurse#)#;#final#int#reclength#=#recmatches#.#getLength#(#)#;##for#(#int#j#=#0#;##j#<#reclength#;#j#++#)#{#matches#.#addElement#(#recmatches#.#item#(#i#)#)#;#}#}#}#}##return#matches#;#}