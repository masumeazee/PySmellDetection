Element#getChildByTagName#(#Node#parent#,#String#tagname#)#{##if#(#parent#==#null#)#{##return#null#;#}##NodeList#childList#=#parent#.#getChildNodes#(#)#;#final#int#len#=#childList#.#getLength#(#)#;##for#(#int#i#=#0#;##i#<#len#;#i#++#)#{#Node#child#=#childList#.#item#(#i#)#;##if#(#child#!=#null#&&#child#.#getNodeType#(#)#==#Node#.#ELEMENT_NODE#&&#child#.#getNodeName#(#)#.#equals#(#tagname#)#)#{##return#(#Element#)#child#;#}#}##return#null#;#}