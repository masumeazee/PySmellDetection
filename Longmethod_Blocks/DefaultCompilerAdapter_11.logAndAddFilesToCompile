void#logAndAddFilesToCompile#(#Commandline#cmd#)#{#attributes#.#log#(#"Compilation "#+#cmd#.#describeArguments#(#)#,#Project#.#MSG_VERBOSE#)#;#StringBuffer#niceSourceList#=#new#StringBuffer#(#"File"#)#;##if#(#compileList#.#length#!=#1#)#{#niceSourceList#.#append#(#"s"#)#;#}##niceSourceList#.#append#(#" to be compiled:"#)#;#niceSourceList#.#append#(#StringUtils#.#LINE_SEP#)#;##for#(#int#i#=#0#;##i#<#compileList#.#length#;#i#++#)#{#String#arg#=#compileList#[#i#]#.#getAbsolutePath#(#)#;#cmd#.#createArgument#(#)#.#setValue#(#arg#)#;#niceSourceList#.#append#(#"    "#)#;#niceSourceList#.#append#(#arg#)#;#niceSourceList#.#append#(#StringUtils#.#LINE_SEP#)#;#}##attributes#.#log#(#niceSourceList#.#toString#(#)#,#Project#.#MSG_VERBOSE#)#;#}