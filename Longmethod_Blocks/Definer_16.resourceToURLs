Enumeration##/*<URL>*/#resourceToURLs#(#ClassLoader#classLoader#)#{#Enumeration#ret#;##try#{#ret#=#classLoader#.#getResources#(#resource#)#;#}##catch#(#IOException#e#)#{#throw#new#BuildException#(#"Could not fetch resources named "#+#resource#,#e#,#getLocation#(#)#)#;#}##if#(#!#ret#.#hasMoreElements#(#)#)#{#String#message#=#"Could not load definitions from resource "#+#resource#+#". It could not be found."#;##switch#(#onError#)#{#case#OnError#.#FAIL_ALL#:#throw#new#BuildException#(#message#)#;#case#OnError#.#FAIL#:#case#OnError#.#REPORT#:#log#(#message#,#Project#.#MSG_WARN#)#;#break#;#case#OnError#.#IGNORE#:#log#(#message#,#Project#.#MSG_VERBOSE#)#;#break#;#default#:##// Ignore the problem#break#;#}#}##return#ret#;#}