boolean#shouldSkipPattern#(#String#pattern#)#{##if#(#FileUtils#.#isAbsolutePath#(#pattern#)#)#{##//skip abs. paths not under basedir, if set:##if#(#basedir#!=#null#&&#!#SelectorUtils#.#matchPatternStart#(#pattern#,#basedir#.#getAbsolutePath#(#)#,#isCaseSensitive#(#)#)#)#{##return#true#;#}#}#else##if#(#basedir#==#null#)#{##//skip non-abs. paths if basedir == null:##return#true#;#}##return#false#;#}