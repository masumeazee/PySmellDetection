void#processIncluded#(#TokenizedPath#path#,#File#file#,#Vector#inc#,#Vector#exc#,#Vector#des#)#{#String#name#=#path#.#toString#(#)#;##if#(#inc#.#contains#(#name#)#||#exc#.#contains#(#name#)#||#des#.#contains#(#name#)#)#{##return#;#}##boolean#included#=#false#;##if#(#isExcluded#(#path#)#)#{#exc#.#add#(#name#)#;#}#else##if#(#isSelected#(#name#,#file#)#)#{#included#=#true#;#inc#.#add#(#name#)#;#}#else#{#des#.#add#(#name#)#;#}##everythingIncluded#&=#included#;#}