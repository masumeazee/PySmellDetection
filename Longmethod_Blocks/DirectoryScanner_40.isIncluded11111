boolean#isIncluded#(#TokenizedPath#path#)#{#ensureNonPatternSetsReady#(#)#;##if#(#isCaseSensitive#(#)#?#includeNonPatterns#.#containsKey#(#path#.#toString#(#)#)#:#includeNonPatterns#.#containsKey#(#path#.#toString#(#)#.#toUpperCase#(#)#)#)#{##return#true#;#}##for#(#int#i#=#0#;##i#<#includePatterns#.#length#;#i#++#)#{##if#(#includePatterns#[#i#]#.#matchPath#(#path#,#isCaseSensitive#(#)#)#)#{##return#true#;#}#}##return#false#;#}