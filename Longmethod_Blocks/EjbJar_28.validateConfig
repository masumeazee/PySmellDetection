void#validateConfig#(#)#throws#BuildException#{##if#(#config#.#srcDir#==#null#)#{#throw#new#BuildException#(#"The srcDir attribute must be specified"#)#;#}##if#(#config#.#descriptorDir#==#null#)#{#config#.#descriptorDir#=#config#.#srcDir#;#}##if#(#config#.#namingScheme#==#null#)#{#config#.#namingScheme#=#new#NamingScheme#(#)#;#config#.#namingScheme#.#setValue#(#NamingScheme#.#DESCRIPTOR#)#;#}#else##if#(#config#.#namingScheme#.#getValue#(#)#.#equals#(#NamingScheme#.#BASEJARNAME#)#&&#config#.#baseJarName#==#null#)#{#throw#new#BuildException#(#"The basejarname attribute must "#+#"be specified with the basejarname naming scheme"#)#;#}#}