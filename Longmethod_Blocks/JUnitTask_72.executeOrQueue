Collection#executeOrQueue#(#Enumeration#testList#,#boolean#runIndividual#)#{#Map#testConfigurations#=#new#HashMap#(#)#;##while#(#testList#.#hasMoreElements#(#)#)#{#JUnitTest#test#=#(#JUnitTest#)#testList#.#nextElement#(#)#;##if#(#test#.#shouldRun#(#getProject#(#)#)#)#{##if#(#runIndividual#||#!#test#.#getFork#(#)#)#{#execute#(#test#)#;#}#else#{#ForkedTestConfiguration#c#=#new#ForkedTestConfiguration#(#test#)#;#List#l#=#(#List#)#testConfigurations#.#get#(#c#)#;##if#(#l#==#null#)#{#l#=#new#ArrayList#(#)#;#testConfigurations#.#put#(#c#,#l#)#;#}##l#.#add#(#test#)#;#}#}#}##return#testConfigurations#.#values#(#)#;#}