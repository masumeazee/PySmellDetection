void#testNoTestCase#(#)#{#TestRunner#runner#=#createRunner#(#NoTestCase#.#class#)#;#runner#.#run#(#)#;##// On junit3 this is a FAILURE, on junit4 this is an ERROR#int#ret#=#runner#.#getRetCode#(#)#;##if#(#ret#!=#JUnitTestRunner#.#FAILURES#&&#ret#!=#JUnitTestRunner#.#ERRORS#)#{#fail#(#"Unexpected result "#+#ret#+#" from junit runner"#)#;#}##// JUnit3 test##//assertEquals(runner.getFormatter().getError(), JUnitTestRunner.FAILURES, runner.getRetCode());#}