int#getCompilerOptions#(#int#options#)#{#int#cOptions#=#Perl5Compiler#.#DEFAULT_MASK#;##if#(#RegexpUtil#.#hasFlag#(#options#,#MATCH_CASE_INSENSITIVE#)#)#{#cOptions#|=#Perl5Compiler#.#CASE_INSENSITIVE_MASK#;#}##if#(#RegexpUtil#.#hasFlag#(#options#,#MATCH_MULTILINE#)#)#{#cOptions#|=#Perl5Compiler#.#MULTILINE_MASK#;#}##if#(#RegexpUtil#.#hasFlag#(#options#,#MATCH_SINGLELINE#)#)#{#cOptions#|=#Perl5Compiler#.#SINGLELINE_MASK#;#}##return#cOptions#;#}