void#writeExternalArgs#(#Commandline#toExecute#)#{##// If using an external file, write the command line options to it#File#optionsTmpFile#=#null#;#BufferedWriter#optionsListWriter#=#null#;##try#{#optionsTmpFile#=#FILE_UTILS#.#createTempFile#(#"javadocOptions"#,#""#,#null#,#true#,#true#)#;#String#[#]#listOpt#=#toExecute#.#getArguments#(#)#;#toExecute#.#clearArgs#(#)#;#toExecute#.#createArgument#(#)#.#setValue#(#"@"#+#optionsTmpFile#.#getAbsolutePath#(#)#)#;#optionsListWriter#=#new#BufferedWriter#(#new#FileWriter#(#optionsTmpFile#.#getAbsolutePath#(#)#,#true#)#)#;##for#(#int#i#=#0#;##i#<#listOpt#.#length#;#i#++#)#{#String#string#=#listOpt#[#i#]#;##if#(#string#.#startsWith#(#"-J-"#)#)#{#toExecute#.#createArgument#(#)#.#setValue#(#string#)#;#}#else#{##if#(#string#.#startsWith#(#"-"#)#)#{#optionsListWriter#.#write#(#string#)#;#optionsListWriter#.#write#(#" "#)#;#}#else#{#optionsListWriter#.#write#(#quoteString#(#string#)#)#;#optionsListWriter#.#newLine#(#)#;#}#}#}##optionsListWriter#.#close#(#)#;#}##catch#(#IOException#ex#)#{##if#(#optionsTmpFile#!=#null#)#{#optionsTmpFile#.#delete#(#)#;#}##throw#new#BuildException#(#"Error creating or writing temporary file for javadoc options"#,#ex#,#getLocation#(#)#)#;#}##finally#{#FileUtils#.#close#(#optionsListWriter#)#;#}#}