String#getVersionDate#(#)#throws#BuildException#{##if#(#fromDate#==#null#&&#toDate#==#null#&&#numDays#==#Integer#.#MIN_VALUE#)#{##return#""#;#}##if#(#fromDate#!=#null#&&#toDate#!=#null#)#{##return#FLAG_VERSION_DATE#+#toDate#+#VALUE_FROMDATE#+#fromDate#;#}#else##if#(#toDate#!=#null#&&#numDays#!=#Integer#.#MIN_VALUE#)#{#try#{##return#FLAG_VERSION_DATE#+#toDate#+#VALUE_FROMDATE#+#calcDate#(#toDate#,#numDays#)#;#}##catch#(#ParseException#ex#)#{#String#msg#=#"Error parsing date: "#+#toDate#;#throw#new#BuildException#(#msg#,#getLocation#(#)#)#;#}#}#else##if#(#fromDate#!=#null#&&#numDays#!=#Integer#.#MIN_VALUE#)#{#try#{##return#FLAG_VERSION_DATE#+#calcDate#(#fromDate#,#numDays#)#+#VALUE_FROMDATE#+#fromDate#;#}##catch#(#ParseException#ex#)#{#String#msg#=#"Error parsing date: "#+#fromDate#;#throw#new#BuildException#(#msg#,#getLocation#(#)#)#;#}#}#else#{##return#fromDate#!=#null#?#FLAG_VERSION#+#VALUE_FROMDATE#+#fromDate#:#FLAG_VERSION_DATE#+#toDate#;#}#}