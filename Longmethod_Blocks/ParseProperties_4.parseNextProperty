Object#parseNextProperty#(#String#value#,#ParsePosition#pos#)#{#final#int#start#=#pos#.#getIndex#(#)#;##if#(#start#>#value#.#length#(#)#)#{##// early exit, can't find any property here, no need to##// consult all the delegates.##return#null#;#}##String#propertyName#=#parsePropertyName#(#value#,#pos#)#;##if#(#propertyName#!=#null#)#{#Object#result#=#getProperty#(#propertyName#)#;##if#(#result#!=#null#)#{##return#result#;#}##if#(#project#!=#null#)#{#project#.#log#(#"Property \""#+#propertyName#+#"\" has not been set"#,#Project#.#MSG_VERBOSE#)#;#}##return#value#.#substring#(#start#,#pos#.#getIndex#(#)#)#;#}##return#null#;#}