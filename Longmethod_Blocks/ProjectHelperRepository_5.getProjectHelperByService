ProjectHelper#getProjectHelperByService#(#InputStream#is#)#{#try#{##// This code is needed by EBCDIC and other strange systems.##// It's a fix for bugs reported in xerces#InputStreamReader#isr#;##try#{#isr#=#new#InputStreamReader#(#is#,#"UTF-8"#)#;#}##catch#(#java#.#io#.#UnsupportedEncodingException#e#)#{#isr#=#new#InputStreamReader#(#is#)#;#}##BufferedReader#rd#=#new#BufferedReader#(#isr#)#;#String#helperClassName#=#rd#.#readLine#(#)#;#rd#.#close#(#)#;##if#(#helperClassName#!=#null#&&#!#""#.#equals#(#helperClassName#)#)#{##return#newHelper#(#helperClassName#)#;#}#}##catch#(#Exception#e#)#{#System#.#out#.#println#(#"Unable to load ProjectHelper from service "#+#ProjectHelper#.#SERVICE_ID#+#" ("#+#e#.#getMessage#(#)#+#")"#)#;##if#(#DEBUG#)#{#e#.#printStackTrace#(#System#.#err#)#;#}#}##return#null#;#}