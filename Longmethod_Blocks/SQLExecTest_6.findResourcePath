String#findResourcePath#(#String#resource#)#{#resource#=#resource#.#replace#(#'.'#,#'/'#)#+#".class"#;#URL#url#=#getClass#(#)#.#getClassLoader#(#)#.#getResource#(#resource#)#;##if#(#url#==#null#)#{##return#null#;#}##String#u#=#url#.#toString#(#)#;##if#(#u#.#startsWith#(#"jar:file:"#)#)#{#int#pling#=#u#.#indexOf#(#"!"#)#;##return#u#.#substring#(#"jar:file:"#.#length#(#)#,#pling#)#;#}#else##if#(#u#.#startsWith#(#"file:"#)#)#{#int#tail#=#u#.#indexOf#(#resource#)#;##return#u#.#substring#(#"file:"#.#length#(#)#,#tail#)#;#}##return#null#;#}