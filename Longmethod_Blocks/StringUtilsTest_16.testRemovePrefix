void#testRemovePrefix#(#)#{#String#prefix#=#"Prefix"#;#String#name#=#"Name"#;#String#suffix#=#"Suffix"#;#String#input#=#prefix#+#name#+#suffix#;#assertEquals#(#"Does not remove the prefix right."#,#name#+#suffix#,#StringUtils#.#removePrefix#(#input#,#prefix#)#)#;#assertEquals#(#"Should leave the string unattended."#,#prefix#+#name#+#suffix#,#StringUtils#.#removePrefix#(#input#,#"bla"#)#)#;#}