boolean#isDanglingSymbolicLink#(#File#parent#,#String#name#)#throws#IOException#{#File#f#=#new#File#(#parent#,#name#)#;##if#(#!#f#.#exists#(#)#)#{#final#String#localName#=#f#.#getName#(#)#;#String#[#]#c#=#parent#.#list#(#new#FilenameFilter#(#)#{#public#boolean#accept#(#File#d#,#String#n#)#{##return#localName#.#equals#(#n#)#;#}#}#)#;##return#c#!=#null#&&#c#.#length#>#0#;#}##return#false#;#}