boolean#shouldProcess#(#File#f#)#{##if#(#f#.#isFile#(#)#)#{##return#true#;#}##if#(#!#create#)#{##return#false#;#}##Exception#exception#=#null#;##try#{##if#(#FILE_UTILS#.#createNewFile#(#f#,#mkdirs#)#)#{##return#true#;#}#}##catch#(#IOException#e#)#{#exception#=#e#;#}##String#msg#=#"Unable to create "#+#f#;##if#(#exception#==#null#)#{#log#(#msg#,#Project#.#MSG_WARN#)#;##return#false#;#}##throw#new#BuildException#(#msg#,#exception#)#;#}