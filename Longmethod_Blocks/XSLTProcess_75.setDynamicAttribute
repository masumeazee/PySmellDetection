void#setDynamicAttribute#(#String#name#,#String#value#)#throws#BuildException#{##// only 'name' and 'value' exist.##if#(#"name"#.#equalsIgnoreCase#(#name#)#)#{#this#.#name#=#value#;#}#else##if#(#"value"#.#equalsIgnoreCase#(#name#)#)#{##// a value must be of a given type##// say boolean|integer|string that are mostly used.##if#(#"true"#.#equalsIgnoreCase#(#value#)#)#{#this#.#value#=#Boolean#.#TRUE#;#}#else##if#(#"false"#.#equalsIgnoreCase#(#value#)#)#{#this#.#value#=#Boolean#.#FALSE#;#}#else#{#try#{#this#.#value#=#new#Integer#(#value#)#;#}##catch#(#NumberFormatException#e#)#{#this#.#value#=#value#;#}#}#}#else#{#throw#new#BuildException#(#"Unsupported attribute: "#+#name#)#;#}#}