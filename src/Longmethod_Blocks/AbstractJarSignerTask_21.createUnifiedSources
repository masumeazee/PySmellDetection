Vector#createUnifiedSources#(#)#{#Vector#sources#=#(#Vector#)#filesets#.#clone#(#)#;##if#(#jar#!=#null#)#{##//we create a fileset with the source file.##//this lets us combine our logic for handling output directories,##//mapping etc.#FileSet#sourceJar#=#new#FileSet#(#)#;#sourceJar#.#setProject#(#getProject#(#)#)#;#sourceJar#.#setFile#(#jar#)#;#sourceJar#.#setDir#(#jar#.#getParentFile#(#)#)#;#sources#.#add#(#sourceJar#)#;#}##return#sources#;#}