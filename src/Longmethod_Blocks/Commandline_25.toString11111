String#toString#(#String#[#]#line#)#{##// empty path return empty string##if#(#line#==#null#||#line#.#length#==#0#)#{##return#""#;#}##// path containing one or more elements#final#StringBuffer#result#=#new#StringBuffer#(#)#;##for#(#int#i#=#0#;##i#<#line#.#length#;#i#++#)#{##if#(#i#>#0#)#{#result#.#append#(#' '#)#;#}##result#.#append#(#quoteArgument#(#line#[#i#]#)#)#;#}##return#result#.#toString#(#)#;#}