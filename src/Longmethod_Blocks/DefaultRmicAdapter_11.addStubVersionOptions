String#addStubVersionOptions#(#)#{##//handle the many different stub options.#String#stubVersion#=#attributes#.#getStubVersion#(#)#;##//default is compatibility#String#stubOption#=#null#;##if#(#null#!=#stubVersion#)#{##if#(#STUB_OPTION_1_1#.#equals#(#stubVersion#)#)#{#stubOption#=#STUB_1_1#;#}#else##if#(#STUB_OPTION_1_2#.#equals#(#stubVersion#)#)#{#stubOption#=#STUB_1_2#;#}#else##if#(#STUB_OPTION_COMPAT#.#equals#(#stubVersion#)#)#{#stubOption#=#STUB_COMPAT#;#}#else#{##//anything else#attributes#.#log#(#"Unknown stub option "#+#stubVersion#)#;##//do nothing with the value? or go -v+stubVersion??#}#}##//for java1.5+, we generate compatible stubs, that is, unless##//the caller asked for IDL or IIOP support.##if#(#stubOption#==#null#&&#!#attributes#.#getIiop#(#)#&&#!#attributes#.#getIdl#(#)#)#{#stubOption#=#STUB_COMPAT#;#}##return#stubOption#;#}