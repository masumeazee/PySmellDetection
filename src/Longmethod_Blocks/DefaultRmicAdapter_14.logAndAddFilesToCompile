void#logAndAddFilesToCompile#(#Commandline#cmd#)#{#Vector#compileList#=#attributes#.#getCompileList#(#)#;#attributes#.#log#(#"Compilation "#+#cmd#.#describeArguments#(#)#,#Project#.#MSG_VERBOSE#)#;#StringBuffer#niceSourceList#=#new#StringBuffer#(#"File"#)#;#int#cListSize#=#compileList#.#size#(#)#;##if#(#cListSize#!=#1#)#{#niceSourceList#.#append#(#"s"#)#;#}##niceSourceList#.#append#(#" to be compiled:"#)#;##for#(#int#i#=#0#;##i#<#cListSize#;#i#++#)#{#String#arg#=#(#String#)#compileList#.#elementAt#(#i#)#;#cmd#.#createArgument#(#)#.#setValue#(#arg#)#;#niceSourceList#.#append#(#"    "#)#;#niceSourceList#.#append#(#arg#)#;#}##attributes#.#log#(#niceSourceList#.#toString#(#)#,#Project#.#MSG_VERBOSE#)#;#}