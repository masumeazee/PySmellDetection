void#doReportLocale#(#PrintStream#out#)#{##//calendar stuff.#Calendar#cal#=#Calendar#.#getInstance#(#)#;#TimeZone#tz#=#cal#.#getTimeZone#(#)#;#out#.#println#(#"Timezone "#+#tz#.#getDisplayName#(#)#+#" offset="#+#tz#.#getOffset#(#cal#.#get#(#Calendar#.#ERA#)#,#cal#.#get#(#Calendar#.#YEAR#)#,#cal#.#get#(#Calendar#.#MONTH#)#,#cal#.#get#(#Calendar#.#DAY_OF_MONTH#)#,#cal#.#get#(#Calendar#.#DAY_OF_WEEK#)#,#(#(#cal#.#get#(#Calendar#.#HOUR_OF_DAY#)#*#MINUTES_PER_HOUR#+#cal#.#get#(#Calendar#.#MINUTE#)#)#*#SECONDS_PER_MINUTE#+#cal#.#get#(#Calendar#.#SECOND#)#)#*#SECONDS_PER_MILLISECOND#+#cal#.#get#(#Calendar#.#MILLISECOND#)#)#)#;#}