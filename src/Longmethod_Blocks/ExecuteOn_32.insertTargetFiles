void#insertTargetFiles#(#String#[#]#targetFiles#,#String#[#]#arguments#,#int#insertPosition#,#String#prefix#,#String#suffix#)#{##if#(#prefix#.#length#(#)#==#0#&&#suffix#.#length#(#)#==#0#)#{#System#.#arraycopy#(#targetFiles#,#0#,#arguments#,#insertPosition#,#targetFiles#.#length#)#;#}#else#{#for#(#int#i#=#0#;##i#<#targetFiles#.#length#;#i#++#)#{#arguments#[#insertPosition#+#i#]#=#prefix#+#targetFiles#[#i#]#+#suffix#;#}#}#}