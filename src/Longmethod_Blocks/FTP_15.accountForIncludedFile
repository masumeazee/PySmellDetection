void#accountForIncludedFile#(#String#name#)#{##if#(#!#filesIncluded#.#contains#(#name#)#&&#!#filesExcluded#.#contains#(#name#)#)#{##if#(#isIncluded#(#name#)#)#{##if#(#!#isExcluded#(#name#)#&&#isSelected#(#name#,#(#File#)#scannedDirs#.#get#(#name#)#)#)#{#filesIncluded#.#addElement#(#name#)#;#}#else#{#filesExcluded#.#addElement#(#name#)#;#}#}#else#{#filesNotIncluded#.#addElement#(#name#)#;#}#}#}