boolean#isContextRelativePath#(#String#filename#)#{##if#(#!#(#ON_DOS#||#ON_NETWARE#)#||#filename#.#length#(#)#==#0#)#{##return#false#;#}##char#sep#=#File#.#separatorChar#;#filename#=#filename#.#replace#(#'/'#,#sep#)#.#replace#(#'\\'#,#sep#)#;#char#c#=#filename#.#charAt#(#0#)#;#int#len#=#filename#.#length#(#)#;##return#(#c#==#sep#&&#(#len#==#1#||#filename#.#charAt#(#1#)#!=#sep#)#)#||#(#Character#.#isLetter#(#c#)#&&#len#>#1#&&#filename#.#indexOf#(#':'#)#==#1#&&#(#len#==#2#||#filename#.#charAt#(#2#)#!=#sep#)#)#;#}