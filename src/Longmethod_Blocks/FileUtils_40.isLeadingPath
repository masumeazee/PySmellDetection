boolean#isLeadingPath#(#File#leading#,#File#path#)#{#String#l#=#normalize#(#leading#.#getAbsolutePath#(#)#)#.#getAbsolutePath#(#)#;#String#p#=#normalize#(#path#.#getAbsolutePath#(#)#)#.#getAbsolutePath#(#)#;##if#(#l#.#equals#(#p#)#)#{##return#true#;#}##// ensure that l ends with a /##// so we never think /foo was a parent directory of /foobar##if#(#!#l#.#endsWith#(#File#.#separator#)#)#{#l#+=#File#.#separator#;#}##return#p#.#startsWith#(#l#)#;#}