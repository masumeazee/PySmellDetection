boolean#hasErrorInCase#(#File#localFile#)#{#localFile#=#normalize#(#localFile#.#getAbsolutePath#(#)#)#;##if#(#!#localFile#.#exists#(#)#)#{##return#false#;#}##final#String#localFileName#=#localFile#.#getName#(#)#;#FilenameFilter#ff#=#new#FilenameFilter#(#)#{#public#boolean#accept#(#File#dir#,#String#name#)#{##return#name#.#equalsIgnoreCase#(#localFileName#)#&&#(#!#name#.#equals#(#localFileName#)#)#;#}#}#;#String#[#]#names#=#localFile#.#getParentFile#(#)#.#list#(#ff#)#;##return#names#!=#null#&&#names#.#length#==#1#;#}