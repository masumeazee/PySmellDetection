String#replaceInvalidChars#(#final#String#name#)#{#StringBuffer#sb#=#new#StringBuffer#(#)#;#final#int#len#=#name#.#length#(#)#;#boolean#changes#=#false#;##for#(#int#i#=#0#;##i#<#len#;#i#++#)#{#final#char#ch#=#name#.#charAt#(#i#)#;##if#(#ManifestTask#.#VALID_ATTRIBUTE_CHARS#.#indexOf#(#ch#)#<#0#)#{#sb#.#append#(#"_"#)#;#changes#=#true#;#}#else#{#sb#.#append#(#ch#)#;#}#}##return#changes#?#sb#.#toString#(#)#:#name#;#}