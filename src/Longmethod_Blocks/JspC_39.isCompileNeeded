boolean#isCompileNeeded#(#File#srcFile#,#File#javaFile#)#{#boolean#shouldCompile#=#false#;##if#(#!#javaFile#.#exists#(#)#)#{#shouldCompile#=#true#;#log#(#"Compiling "#+#srcFile#.#getPath#(#)#+#" because java file "#+#javaFile#.#getPath#(#)#+#" does not exist"#,#Project#.#MSG_VERBOSE#)#;#}#else#{##if#(#srcFile#.#lastModified#(#)#>#javaFile#.#lastModified#(#)#)#{#shouldCompile#=#true#;#log#(#"Compiling "#+#srcFile#.#getPath#(#)#+#" because it is out of date with respect to "#+#javaFile#.#getPath#(#)#,#Project#.#MSG_VERBOSE#)#;#}#else#{##if#(#javaFile#.#length#(#)#==#0#)#{#shouldCompile#=#true#;#log#(#"Compiling "#+#srcFile#.#getPath#(#)#+#" because java file "#+#javaFile#.#getPath#(#)#+#" is empty"#,#Project#.#MSG_VERBOSE#)#;#}#}#}##return#shouldCompile#;#}