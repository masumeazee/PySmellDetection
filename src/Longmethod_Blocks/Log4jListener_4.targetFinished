void#targetFinished#(#BuildEvent#event#)#{##if#(#initialized#)#{#String#targetName#=#event#.#getTarget#(#)#.#getName#(#)#;#Logger#cat#=#Logger#.#getLogger#(#Target#.#class#.#getName#(#)#)#;##if#(#event#.#getException#(#)#==#null#)#{#cat#.#info#(#"Target \""#+#targetName#+#"\" finished."#)#;#}#else#{#cat#.#error#(#"Target \""#+#targetName#+#"\" finished with error."#,#event#.#getException#(#)#)#;#}#}#}