String#getVersionDateLabel#(#)#{#String#versionDateLabel#=#""#;##if#(#version#!=#null#)#{#versionDateLabel#=#FLAG_VERSION#+#version#;#}#else##if#(#date#!=#null#)#{#versionDateLabel#=#FLAG_VERSION_DATE#+#date#;#}#else#{##// Use getShortLabel() so labels longer then 30 char are truncated##// and the user is warned#String#shortLabel#=#getShortLabel#(#)#;##if#(#shortLabel#!=#null#&&#!#shortLabel#.#equals#(#""#)#)#{#versionDateLabel#=#FLAG_VERSION_LABEL#+#shortLabel#;#}#}##return#versionDateLabel#;#}