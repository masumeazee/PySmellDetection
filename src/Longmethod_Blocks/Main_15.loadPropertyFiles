void#loadPropertyFiles#(#)#{#for#(#int#propertyFileIndex#=#0#;##propertyFileIndex#<#propertyFiles#.#size#(#)#;#propertyFileIndex#++#)#{#String#filename#=#(#String#)#propertyFiles#.#elementAt#(#propertyFileIndex#)#;#Properties#props#=#new#Properties#(#)#;#FileInputStream#fis#=#null#;##try#{#fis#=#new#FileInputStream#(#filename#)#;#props#.#load#(#fis#)#;#}##catch#(#IOException#e#)#{#System#.#out#.#println#(#"Could not load property file "#+#filename#+#": "#+#e#.#getMessage#(#)#)#;#}##finally#{#FileUtils#.#close#(#fis#)#;#}##// ensure that -D properties take precedence#Enumeration#propertyNames#=#props#.#propertyNames#(#)#;##while#(#propertyNames#.#hasMoreElements#(#)#)#{#String#name#=#(#String#)#propertyNames#.#nextElement#(#)#;##if#(#definedProps#.#getProperty#(#name#)#==#null#)#{#definedProps#.#put#(#name#,#props#.#getProperty#(#name#)#)#;#}#}#}#}