void#validateSetup#(#)#throws#BuildException#{##if#(#path#==#null#)#{#throw#new#BuildException#(#"You must specify a path to convert"#)#;#}##// Determine the separator strings.  The dirsep and pathsep attributes##// override the targetOS settings.#String#dsep#=#File#.#separator#;#String#psep#=#File#.#pathSeparator#;##if#(#targetOS#!=#null#)#{#psep#=#targetWindows#?#";"#:#":"#;#dsep#=#targetWindows#?#"\\"#:#"/"#;#}##if#(#pathSep#!=#null#)#{##// override with pathsep=#psep#=#pathSep#;#}##if#(#dirSep#!=#null#)#{##// override with dirsep=#dsep#=#dirSep#;#}##pathSep#=#psep#;#dirSep#=#dsep#;#}