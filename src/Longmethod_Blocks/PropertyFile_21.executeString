void#executeString#(#String#oldValue#)#throws#BuildException#{#String#newV#=#DEFAULT_STRING_VALUE#;#String#currentValue#=#getCurrentValue#(#oldValue#)#;##if#(#currentValue#==#null#)#{#currentValue#=#DEFAULT_STRING_VALUE#;#}##if#(#operation#==#Operation#.#EQUALS_OPER#)#{#newV#=#currentValue#;#}#else##if#(#operation#==#Operation#.#INCREMENT_OPER#)#{#newV#=#currentValue#+#value#;#}##this#.#newValue#=#newV#;#}