String#guessRpmBuildCommand#(#)#{#Vector#env#=#Execute#.#getProcEnvironment#(#)#;#String#path#=#null#;##for#(#Enumeration#e#=#env#.#elements#(#)#;##e#.#hasMoreElements#(#)#;#)#{#String#var#=#(#String#)#e#.#nextElement#(#)#;##if#(#var#.#startsWith#(#PATH1#)#||#var#.#startsWith#(#PATH2#)#||#var#.#startsWith#(#PATH3#)#)#{#path#=#var#.#substring#(#PATH_LEN#)#;#break#;#}#}##if#(#path#!=#null#)#{#Path#p#=#new#Path#(#getProject#(#)#,#path#)#;#String#[#]#pElements#=#p#.#list#(#)#;##for#(#int#i#=#0#;##i#<#pElements#.#length#;#i#++#)#{#File#f#=#new#File#(#pElements#[#i#]#,#"rpmbuild"#+#(#Os#.#isFamily#(#"dos"#)#?#".exe"#:#""#)#)#;##if#(#f#.#canRead#(#)#)#{##return#f#.#getAbsolutePath#(#)#;#}#}#}##return#"rpm"#;#}