PlanarImage#executeTransformOperation#(#PlanarImage#image#)#{#BufferedImage#bi#=#null#;##for#(#int#i#=#0#;##i#<#instructions#.#size#(#)#;#i#++#)#{#ImageOperation#instr#=#(#(#ImageOperation#)#instructions#.#elementAt#(#i#)#)#;##if#(#instr#instanceof#DrawOperation#)#{##return#performScale#(#image#)#;#}#else##if#(#instr#instanceof#TransformOperation#)#{#bi#=#image#.#getAsBufferedImage#(#)#;#image#=#(#(#TransformOperation#)#instr#)#.#executeTransformOperation#(#PlanarImage#.#wrapRenderedImage#(#bi#)#)#;#bi#=#image#.#getAsBufferedImage#(#)#;#}#}##return#performScale#(#image#)#;#}