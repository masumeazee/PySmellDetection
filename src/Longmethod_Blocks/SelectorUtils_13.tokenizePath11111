Vector#tokenizePath#(#String#path#,#String#separator#)#{#Vector#ret#=#new#Vector#(#)#;##if#(#FileUtils#.#isAbsolutePath#(#path#)#)#{#String#[#]#s#=#FILE_UTILS#.#dissect#(#path#)#;#ret#.#add#(#s#[#0#]#)#;#path#=#s#[#1#]#;#}##StringTokenizer#st#=#new#StringTokenizer#(#path#,#separator#)#;##while#(#st#.#hasMoreTokens#(#)#)#{#ret#.#addElement#(#st#.#nextToken#(#)#)#;#}##return#ret#;#}