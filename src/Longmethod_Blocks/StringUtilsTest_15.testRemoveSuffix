void#testRemoveSuffix#(#)#{#String#prefix#=#"Prefix"#;#String#name#=#"Name"#;#String#suffix#=#"Suffix"#;#String#input#=#prefix#+#name#+#suffix#;#assertEquals#(#"Does not remove the suffix right."#,#prefix#+#name#,#StringUtils#.#removeSuffix#(#input#,#suffix#)#)#;#assertEquals#(#"Should leave the string unattended."#,#prefix#+#name#+#suffix#,#StringUtils#.#removeSuffix#(#input#,#"bla"#)#)#;#}