File#findFile#(#File#base#,#final#boolean#cs#)#{#String#[#]#tokens#=#tokenizedPath#;##if#(#FileUtils#.#isAbsolutePath#(#path#)#)#{##if#(#base#==#null#)#{#String#[#]#s#=#FILE_UTILS#.#dissect#(#path#)#;#base#=#new#File#(#s#[#0#]#)#;#tokens#=#SelectorUtils#.#tokenizePathAsArray#(#s#[#1#]#)#;#}#else#{#File#f#=#FILE_UTILS#.#normalize#(#path#)#;#String#s#=#FILE_UTILS#.#removeLeadingPath#(#base#,#f#)#;##if#(#s#.#equals#(#f#.#getAbsolutePath#(#)#)#)#{##//removing base from path yields no change; path##//not child of base##return#null#;#}##tokens#=#SelectorUtils#.#tokenizePathAsArray#(#s#)#;#}#}##return#findFile#(#base#,#tokens#,#cs#)#;#}