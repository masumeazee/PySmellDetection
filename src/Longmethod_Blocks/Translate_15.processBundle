void#processBundle#(#final#String#bundleFile#,#final#int#i#,#final#boolean#checkLoaded#)#throws#BuildException#{#final#File#propsFile#=#getProject#(#)#.#resolveFile#(#bundleFile#+#".properties"#)#;#FileInputStream#ins#=#null#;##try#{#ins#=#new#FileInputStream#(#propsFile#)#;#loaded#=#true#;#bundleLastModified#[#i#]#=#propsFile#.#lastModified#(#)#;#log#(#"Using "#+#propsFile#,#Project#.#MSG_DEBUG#)#;#loadResourceMap#(#ins#)#;#}##catch#(#IOException#ioe#)#{#log#(#propsFile#+#" not found."#,#Project#.#MSG_DEBUG#)#;##//if all resource files associated with this bundle##//have been scanned for and still not able to##//find a single resrouce file, throw exception##if#(#!#loaded#&&#checkLoaded#)#{#throw#new#BuildException#(#ioe#.#getMessage#(#)#,#getLocation#(#)#)#;#}#}#}