void#expandResource#(#Resource#srcR#,#File#dir#)#{##if#(#!#srcR#.#isExists#(#)#)#{#throw#new#BuildException#(#"Unable to untar "#+#srcR#.#getName#(#)#+#" as the it does not exist"#,#getLocation#(#)#)#;#}##InputStream#i#=#null#;##try#{#i#=#srcR#.#getInputStream#(#)#;#expandStream#(#srcR#.#getName#(#)#,#i#,#dir#)#;#}##catch#(#IOException#ioe#)#{#throw#new#BuildException#(#"Error while expanding "#+#srcR#.#getName#(#)#,#ioe#,#getLocation#(#)#)#;#}##finally#{#FileUtils#.#close#(#i#)#;#}#}