void#addVendorFiles#(#Hashtable#ejbFiles#,#String#ddPrefix#)#{#super#.#addVendorFiles#(#ejbFiles#,#ddPrefix#)#;##// Then the toplink deployment descriptor##// Setup a naming standard here?.#File#toplinkDD#=#new#File#(#getConfig#(#)#.#descriptorDir#,#ddPrefix#+#toplinkDescriptor#)#;##if#(#toplinkDD#.#exists#(#)#)#{#ejbFiles#.#put#(#META_DIR#+#toplinkDescriptor#,#toplinkDD#)#;#}#else#{#log#(#"Unable to locate toplink deployment descriptor. "#+#"It was expected to be in "#+#toplinkDD#.#getPath#(#)#,#Project#.#MSG_WARN#)#;#}#}