InputSource#classpathLookup#(#ResourceLocation#matchingEntry#)#{#InputSource#source#=#null#;#AntClassLoader#loader#=#null#;#Path#cp#=#classpath#;##if#(#cp#!=#null#)#{#cp#=#classpath#.#concatSystemClasspath#(#"ignore"#)#;#}#else#{#cp#=#(#new#Path#(#getProject#(#)#)#)#.#concatSystemClasspath#(#"last"#)#;#}##loader#=#getProject#(#)#.#createClassLoader#(#cp#)#;##//##// for classpath lookup we ignore the base directory##//#InputStream#is#=#loader#.#getResourceAsStream#(#matchingEntry#.#getLocation#(#)#)#;##if#(#is#!=#null#)#{#source#=#new#InputSource#(#is#)#;#URL#entryURL#=#loader#.#getResource#(#matchingEntry#.#getLocation#(#)#)#;#String#sysid#=#entryURL#.#toExternalForm#(#)#;#source#.#setSystemId#(#sysid#)#;#log#(#"catalog entry matched a resource in the classpath: '"#+#sysid#+#"'"#,#Project#.#MSG_DEBUG#)#;#}##return#source#;#}