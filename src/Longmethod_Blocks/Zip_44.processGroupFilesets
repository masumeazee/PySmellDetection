void#processGroupFilesets#(#)#{##// Add the files found in groupfileset to fileset#for#(#int#i#=#0#;##i#<#groupfilesets#.#size#(#)#;#i#++#)#{#logWhenWriting#(#"Processing groupfileset "#,#Project#.#MSG_VERBOSE#)#;#FileSet#fs#=#(#FileSet#)#groupfilesets#.#elementAt#(#i#)#;#FileScanner#scanner#=#fs#.#getDirectoryScanner#(#getProject#(#)#)#;#String#[#]#files#=#scanner#.#getIncludedFiles#(#)#;#File#basedir#=#scanner#.#getBasedir#(#)#;##for#(#int#j#=#0#;##j#<#files#.#length#;#j#++#)#{#logWhenWriting#(#"Adding file "#+#files#[#j#]#+#" to fileset"#,#Project#.#MSG_VERBOSE#)#;#ZipFileSet#zf#=#new#ZipFileSet#(#)#;#zf#.#setProject#(#getProject#(#)#)#;#zf#.#setSrc#(#new#File#(#basedir#,#files#[#j#]#)#)#;#add#(#zf#)#;#filesetsFromGroupfilesets#.#addElement#(#zf#)#;#}#}#}